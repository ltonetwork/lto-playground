{
  "name": "lto-playground",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "npm run build:app && npm run build:bundle && npm run build:clean && npm run build:assets && cat ./src/dist-index.html > ./dist/lto-playground/index.html",
    "build:app": "ng build --aot --prod --build-optimizer --vendor-chunk=false --output-hashing=none",
    "build:bundle": "cd ./dist/lto-playground/ && cat runtime.js >> bundle.js && cat polyfills.js >> bundle.js && cat scripts.js >> bundle.js && cat main.js >> bundle.js",
    "build:clean": "cd ./dist/lto-playground/ && rm -f runtime.js && rm -f polyfills.js && rm -f scripts.js && rm -f main.js",
    "build:assets": "cp -rf ./src/assets ./dist/lto-playground/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^6.1.0",
    "@angular/cdk": "^6.4.1",
    "@angular/common": "^6.1.0",
    "@angular/compiler": "^6.1.0",
    "@angular/core": "^6.1.0",
    "@angular/flex-layout": "^6.0.0-beta.17",
    "@angular/forms": "^6.1.0",
    "@angular/http": "^6.1.0",
    "@angular/material": "^6.4.1",
    "@angular/platform-browser": "^6.1.0",
    "@angular/platform-browser-dynamic": "^6.1.0",
    "@angular/router": "^6.1.0",
    "@ngxs/store": "^3.2.0",
    "core-js": "^2.5.4",
    "d3": "^5.5.0",
    "dagre": "^0.8.2",
    "dagre-d3": "^0.6.1",
    "graphlib": "^2.1.5",
    "lodash": "^4.17.10",
    "monaco-editor": "^0.13.1",
    "rxjs": "^6.0.0",
    "vis": "^4.21.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.1",
    "@angular-devkit/build-ng-packagr": "~0.7.1",
    "@angular/cli": "~6.1.1",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@ngxs/devtools-plugin": "^3.2.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.5.4",
    "codelyzer": "~4.4.2",
    "jasmine-core": "~3.1.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.14",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^1.2.0",
    "ng-packagr": "^4.0.1",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsickle": "0.32.1",
    "tslib": "^1.7.1",
    "tslint": "~5.11.0",
    "typescript": "~2.9.2"
  }
}
